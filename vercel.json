{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "backend/src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/src/app.js",
      "headers": {
        "Cache-Control": "s-maxage=0"
      }
    }
  ]
}
